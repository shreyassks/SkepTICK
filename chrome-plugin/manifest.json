{  
  "manifest_version": 3,  
  "name": "Investment Extension",  
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],  
  "action": {  
    "default_popup": "popup.html"  
  },  
  "background": {  
    "service_worker": "background.js"  
  },
  "content_scripts": [  
        {  
            "matches": ["https://www.youtube.com/watch?v=*"],  
            "js": ["content.js"]  
        }  
    ]  
}  
